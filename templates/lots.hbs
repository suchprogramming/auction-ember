 <h3 class="auction-inline"><span class="label label-default"> Shad-e-Bay</span></h3>
{{#link-to 'new-lot' classNames="btn btn-success auction-button"}}Add Lot{{/link-to}}

<hr>

{{#each lot in model}}
  <table class="table table-bordered">
    <thead>
      <tr>
        <th colspan="5">
          {{#link-to 'lot' lot.id }}{{lot.name}}{{/link-to}}
        </th>
      </tr>
    </thead>
    <tbody>
      <tr>
        {{#each item in lot.items}}
          <td>
            <small>{{item.itemName}}</small>
            {{#link-to ''}}<img {{bind-attr src=item.image}} style="width:96px;length:80px">{{/link-to}}
          </td>
        {{/each}}
      </tr>
    </tbody>
  </table>
{{/each}}
